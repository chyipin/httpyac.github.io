proto < ./hello.proto

GRPC grpcb.in:9000/hello.HelloService/BidiHello
{
  "greeting": "world"
}

{{@streaming

  async function writeStream(){
    sleep(1000);
    grpcStream.write({
      greeting: ', how are you?',
    });
    sleep(1000);
    grpcStream.write({
      greeting: ', I can stream.',
    });
  }
  exports.waitPromise = writeStream();
}}